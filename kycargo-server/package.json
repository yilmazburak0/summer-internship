{
  "name": "kykargo-server",
  "version": "0.0.1",
  "description": "",
  "main": "src/server.js",
  "scripts": {
    "build": "tsc && babel ./src --out-dir dist -x .js,.ts -D --verbose",
    "buildStart": "cross-env NODE_ENV=production npx babel-node dist/server.js",
    "prepare": "npm run snyk-protect",
    "snyk-protect": "snyk-protect",
    "lint": "./node_modules/.bin/eslint .",
    "start": "cross-env NODE_ENV=production babel-node --experimental-modules src/server.js",
    "prodStart": "nodemon --exec npm run start",
    "devStart": "cross-env NODE_ENV=development npx babel-node -x '.ts,.js' src/server.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec npm run devStart",
    "debug": "cross-env dev=1  babel-node --experimental-modules --inspect-brk --trace-warnings  src/server.js",
    "pretest": "eslint --ignore-path .eslintignore .",
    "testModule": "cross-env  nodemon --exec babel-node --experimental-modules src/test.js"
  },
  "license": "ISC",
  "dependencies": {
    "@snyk/protect": "^1.1061.0",
    "axios": "^1.1.3",
    "babel-node": "^0.0.1-security",
    "babel-plugin-module-resolver": "^4.1.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "bwip-js": "^3.1.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "debug": "^4.3.4",
    "delay": "^5.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "firebase-admin": "^9.4.2",
    "formidable": "^1.2.2",
    "helmet": "^6.0.0",
    "jimp": "^0.16.2",
    "jsonwebtoken": "^8.5.1",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4",
    "moment": "^2.29.4",
    "mongoose": "^5.10.5",
    "morgan": "^1.10.0",
    "morgan-body": "^2.6.8",
    "multer": "^1.4.2",
    "nanoid": "^3.1.20",
    "nodemailer": "^6.8.0",
    "nodemon": "^2.0.20",
    "pluralize": "^8.0.0",
    "qs": "^6.11.0",
    "request-ip": "^3.3.0",
    "shortid": "^2.2.16",
    "socket.io": "^4.5.3",
    "url-parse": "^1.5.10",
    "uuid": "^9.0.0",
    "xlsx": "^0.18.5"
  },
  "snyk": true,
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/node": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.9",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "typescript": "^4.9.3"
  },
  "engines": {
    "node": ">=12.17.0"
  }
}